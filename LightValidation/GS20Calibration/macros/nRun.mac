# Setting up verbosity
/tracking/verbose 0
/run/verbose 0
#
# Setting up the source
#
/control/execute macros/neutronSource.mac
#
# GS20 Simulation
#
/analysis/setFileName GS20_Neutron 
/analysis/h1/set 1 100 0  5.0 MeV none # Energy Depostion
/analysis/h1/set 2 100 0  7000 none    # Number of Optical Photons
/analysis/h1/set 3 100 0  7000 none    # Number of Optical Photons
/run/beamOn 100000
